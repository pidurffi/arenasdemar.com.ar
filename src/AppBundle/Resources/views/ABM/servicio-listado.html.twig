Servicios Profesionales
{% endblock %}

{% block titulo_nuevo_item %}Nuevo Servicio{% endblock %}
{% block avanzu_page_content  %}
	{% set nombre_entidad = 'servicio' %}
	{% set nombre_entidad_plural = 'servicios' %}
	{% set iterables = servicios %}
	{% set puede_crear = true %}
	{% set puede_borrar = true %}
